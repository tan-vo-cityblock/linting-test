{
  "name": "sftp-drop-slack-notifier",
  "version": "1.0.0",
  "description": "Publish GCS PubSub Events to Slack",
  "main": "out/index.js",
  "scripts": {
    "deploy-local": "tsc && functions-emulator deploy gcsPubsubSlack --trigger-provider=cloud.storage --trigger-event=object.change --trigger-resource=my-trigger-bucket --source=out",
    "package-zip": "tsc && zip gcsPubsubSlack.zip out/index.js package.json out/settings.json"
  },
  "license": "Proprietary",
  "repository": {
    "url": "https://github.com/cityblock/mixer"
  },
  "devDependencies": {
    "@types/node": "6.14.8",
    "typescript": "2.9.2"
  },
  "dependencies": {
    "@slack/client": "^4.3.1"
  }
}
