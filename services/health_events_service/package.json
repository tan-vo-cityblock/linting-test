{
  "name": "health-events-service",
  "version": "0.0.1",
  "description": "Health Events Service API; providing CRUD operations to the HIE entity",
  "private": true,
  "author": "Cityblock Health",
  "engines": {
    "node": "14.15.1",
    "npm": "6.14.11"
  },
  "scripts": {
    "build": "tsc -p . && npm run copy-schema-to-dist",
    "dev": "NODE_ENV=development nodemon",
    "lint": "node --max_old_space_size=4096 node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
    "lint:fix": "node --max_old_space_size=4096 node_modules/.bin/tslint --project tsconfig.json --config tslint.json --fix",
    "knex": "ts-node -P tsconfig.json node_modules/.bin/knex",
    "migrate:make": "npm run knex -- migrate:make --knexfile=src/models/knexfile.ts",
    "migrate": "npm run knex -- migrate:latest --knexfile=src/models/knexfile.ts",
    "migrate:rollback": "npm run knex -- migrate:rollback --knexfile=src/models/knexfile.ts",
    "start": "node ./dist/src/index.js",
    "seed": "npm run knex -- seed:run --knexfile=src/models/knexfile.ts",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "backfill-hie-messages:dev": "NODE_ENV=development ts-node -P tsconfig.json ./src/scripts/backfill-hie-messages.ts",
    "backfill-hie-messages:staging": "NODE_ENV=staging ts-node -P tsconfig.json ./src/scripts/backfill-hie-messages.ts",
    "backfill-hie-messages:production": "NODE_ENV=production ts-node -P tsconfig.json ./src/scripts/backfill-hie-messages.ts",
    "refresh-facility-map-cache:staging": "NODE_ENV=staging ts-node -P tsconfig.json ./src/scripts/refresh-facility-map-cache.ts",
    "refresh-facility-map-cache:production": "NODE_ENV=production ts-node -P tsconfig.json ./src/scripts/refresh-facility-map-cache.ts",
    "copy-schema-to-dist": "ts-node -P tsconfig.json ./src/scripts/copy-schema-to-dist.ts"
  },
  "dependencies": {
    "@apollo/federation": "0.25.1",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-typescript": "7.13.0",
    "@google-cloud/bigquery": "5.6.0",
    "@types/node": "14.14.37",
    "apollo-server-express": "2.25.1",
    "babel-plugin-const-enum": "1.0.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "knex": "0.95.6",
    "knex-cleaner": "1.3.1",
    "lodash": "4.17.21",
    "nodemon": "2.0.7",
    "pg": "8.6.0",
    "stream-transform": "2.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/jest": "24.0.20",
    "@types/lodash": "4.14.169",
    "@types/uuid": "8.3.0",
    "jest": "26.6.3",
    "node-mocks-http": "1.10.1",
    "prettier": "2.2.1",
    "ts-jest": "25.5.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "6.2.0"
  }
}
