{
  "name": "quality_measure",
  "description": "Quality Measure Service API providing CRUD operations for managing quality measures at Cityblock",
  "version": "0.0.1",
  "private": true,
  "author": "Cityblock Health",
  "engines": {
    "node": "12.13.1"
  },
  "scripts": {
    "gcp-build": "tsc -p .",
    "deploy": "gcloud app deploy app.quality_measure.yaml --project cbh-services-staging",
    "dev": "npm run migrate && NODE_ENV=development nodemon --exec 'ts-node --transpile-only -P tsconfig.json ./src/index.ts'",
    "knex": "ts-node -P tsconfig.json node_modules/.bin/knex",
    "lint": "node --max_old_space_size=4096 node_modules/.bin/tslint \"src/**/*.ts\" --project tsconfig.json --config tslint.json",
    "lint-fix": "node --max_old_space_size=4096 node_modules/.bin/tslint \"src/**/*.ts\" --project tsconfig.json --config tslint.json --fix",
    "migrate": "npm run knex -- migrate:latest --knexfile=src/models/knexfile.ts",
    "migrate:make": "npm run knex -- migrate:make --knexfile=src/models/knexfile.ts",
    "migrate:rollback": "npm run knex -- migrate:rollback --knexfile=src/models/knexfile.ts",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier-check": "prettier --check \"src/**/*.ts\"",
    "prettier-markdown": "prettier --write \"**/*.md\"",
    "seed": "NODE_ENV=test npm run knex -- seed:run --knexfile=src/models/knexfile.ts",
    "start": "node ./dist/src/index.js",
    "test": "NODE_ENV=development node --max_old_space_size=2048 node_modules/.bin/jest --runInBand --logHeapUsage --detectOpenHandles"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.4",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-typescript": "7.7.4",
    "@types/express": "4.17.1",
    "@types/uuid": "7.0.0",
    "@types/uuid-validate": "0.0.1",
    "@types/validator": "10.11.3",
    "@types/lodash": "4.14.141",
    "axios": "0.21.1",
    "babel-plugin-const-enum": "0.0.5",
    "babel-plugin-lodash": "3.3.4",
    "base-64": "0.1.0",
    "body-parser": "1.19.0",
    "damm": "0.0.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "knex": "0.21.15",
    "lodash": "4.17.19",
    "nodemon": "2.0.3",
    "objection": "2.2.14",
    "pg": "8.5.1",
    "sleep-promise": "8.0.1",
    "slugify": "1.3.6",
    "ts-node": "8.5.4",
    "typescript": "3.7.7",
    "typescript-plugin-lodash": "0.1.0",
    "uuid-validate": "0.0.3",
    "uuid": "7.0.2",
    "validator": "11.1.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.20",
    "jest": "25.5.4",
    "prettier": "2.2.1",
    "ts-jest": "25.5.1",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "6.2.0"
  }
}
