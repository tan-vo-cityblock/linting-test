{
  "name": "member-service",
  "description": "Member Service API; providing CRUD operations to the Member entity",
  "version": "0.0.1",
  "private": true,
  "author": "Cityblock Health",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "gcp-build": "tsc -p .",
    "deploy": "gcloud app deploy",
    "dev": "npm run migrate && NODE_ENV=development API_KEY= nodemon --exec 'ts-node --transpile-only -P tsconfig.json ./src/index.ts'",
    "knex": "ts-node -P tsconfig.json node_modules/.bin/knex",
    "lint": "node --max_old_space_size=4096 node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
    "lint-fix": "node --max_old_space_size=4096 node_modules/.bin/tslint --project tsconfig.json --config tslint.json --fix",
    "migrate": "npm run knex -- migrate:latest --knexfile=src/models/knexfile.ts",
    "migrate:make": "npm run knex -- migrate:make --knexfile=src/models/knexfile.ts",
    "migrate:rollback": "npm run knex -- migrate:rollback --knexfile=src/models/knexfile.ts",
    "seed": "NODE_ENV=test npm run knex -- seed:run --knexfile=src/models/knexfile.ts",
    "start": "node ./dist/src/index.js",
    "test": "NODE_ENV=development node --max_old_space_size=2048 node_modules/.bin/jest --runInBand --logHeapUsage --detectOpenHandles",
    "tsc": "tsc -p .",
    "tsc:check": "tsc -p . --noEmit"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.4",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-typescript": "7.7.4",
    "@google-cloud/error-reporting": "2.0.0",
    "@google-cloud/pubsub": "1.5.0",
    "@types/express": "4.17.1",
    "@types/qs": "^6.9.0",
    "@types/uuid": "8.0.0",
    "@types/uuid-validate": "0.0.1",
    "@types/validator": "10.11.3",
    "axios": "0.21.1",
    "babel-plugin-const-enum": "0.0.5",
    "base-64": "0.1.0",
    "body-parser": "1.19.0",
    "damm": "0.0.1",
    "date-fns": "^2.16.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "knex": "0.20.10",
    "knex-cleaner": "^1.3.1",
    "lodash": "4.17.19",
    "nodemon": "2.0.3",
    "objection": "1.6.11",
    "pg": "7.12.1",
    "sleep-promise": "8.0.1",
    "ts-node": "8.5.4",
    "typescript": "3.7.4",
    "uuid-validate": "0.0.3",
    "validator": "11.1.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.20",
    "husky": "3.0.9",
    "jest": "25.5.4",
    "node-mocks-http": "^1.8.1",
    "sinon": "^9.0.2",
    "ts-jest": "25.5.1",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "6.2.0"
  }
}
